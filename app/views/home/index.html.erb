<div style="padding-left:20px">
  <h1>Welcome to Car Challenge App</h1>
  <p>Here You can make more friends.</p>
  <p>You can create cars , search cars and many more ....</p>
</div>
<%if current_user %>
	<% @user.each do |user|%>
	<div style="overflow-x:auto;">
	  <table>
	    <tr>
	      <th>First Name</th>
	      <th>Last Name</th>
	      <th>Email</th>
	      <th>Profile Image</th>
	    </tr>
	    <tr>
	      <td><%= user.first_name%></td>
	      <td><%= user.last_name%></td>
	      <td><%= user.email%></td>
	      <td><img src="<%= user.profile_photo.url %>" alt="<%= user.profile_photo.url %>" border=3 height=100 width=200></img></td>
	    </tr>
	  </table>
	</div>


	<%end%>
<%end%>

